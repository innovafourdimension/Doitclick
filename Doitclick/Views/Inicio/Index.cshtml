
@{
    ViewData["Title"] = "Mi Gestión";
}

<div class="row">
    <div class="col-lg-12">
        <div id="demo-panel-network" class="panel">
            <div class="panel-heading">
                <h3 class="panel-title">
                    <ol class="breadcrumb">
                        <li></li>
                        <li class="active">Mi Gestión</li>
                    </ol>
                </h3>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-sm-12 col-md-12">
                        <table id="table"
                               data-search="true"
                               data-show-refresh="true"
                               data-show-export="true"
                               data-minimum-count-columns="1"
                               data-show-pagination-switch="true"
                               data-pagination="true"
                               data-id-field="Id"
                               data-page-list="[10]"
                               data-show-footer="false"
                               data-side-pagination="server"
                               data-url="/api/flujo-interno/bandeja-tareas"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-5">
    </div>
</div>

@section styles{ 
    <link href="~/Assets/plugins/bootstrap-table/bootstrap-table.min.css" rel="stylesheet" />
}
@section Scripts{
    <script src="~/Assets/plugins/bootstrap-table/bootstrap-table.min.js"></script>
    <script src="~/js/inicio.min.js"></script>

}